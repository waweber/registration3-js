    "@storybook/test": "^8.1.10",
    "@tanstack/react-query": "^5.48.0",
    "@tanstack/react-router": "^1.40.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.0",
    "mobx": "^6.12.4",
    "mobx-react-lite": "^4.0.7",
    "react-dom": "^18.3.1",
