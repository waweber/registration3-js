{
  "name": "@open-event-systems/registration-common",
  "version": "0.1.0",
  "description": "Registration common modules",
  "type": "module",
  "exports": {
    "./*.scss": "./src/*.scss",
    "./*": [
      "./dist/src/*.js",
      "./dist/src/*/index.js"
    ]
  },
  "imports": {
    "#src/*.scss": "./src/*.scss",
    "#src/*": {
      "storybook": [
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts"
      ],
      "webpack": [
        "./src/*.ts",
        "./src/*.tsx",
        "./src/*/index.ts"
      ],
      "default": [
        "./dist/src/*.js",
        "./dist/src/*/index.js",
        "./dist/src/*"
      ]
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build": "tsc --build",
    "build-storybook": "storybook build",
    "webpack": "webpack --mode=production",
    "start": "webpack serve --mode=development"
  },
  "keywords": [],
  "author": "",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "!*.stories.*",
    "**/*.scss",
    "resources/example-logo.svg"
  ],
  "devDependencies": {
    "@mantine/core": "^7.11.0",
